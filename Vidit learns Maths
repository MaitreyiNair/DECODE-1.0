#include <stdio.h>

int algorithim(int a,int b)
 {
     int k = 0;
     int c;
     while(k<b)
     {
         k = k + 1;
         c = a % 10;
         if(c == 0)
            a = a/10;
         else
             a = a - 1;
         
         
       }
     return a;
 }




int main()
{
    int d;
    scanf("%d",&d);
    
    int k;
    scanf("%d",&k);
    
   
    int e = algorithim(d,k);
    printf("%d",e);
}

 




